@model CreateEditProjectViewModel

@{
    ViewBag.Title = "EditProject";
}

<h2>Edit Project</h2>

<form method="post" enctype="multipart/form-data" style="margin-top:50px">
    @Html.ValidationSummary(false)

    <div class="form-group">
        @Html.LabelFor(p => p.Name, "Name")
        @Html.TextBoxFor(p => p.Name, new { @class = "form-control", placeholder = "Name" })
    </div>
    <div class="form-group">        
        <div class="">
            <button type="submit" class="btn btn-default">Submit</button>
        </div>
    </div>
</form>